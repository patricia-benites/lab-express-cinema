<div class="movies-page">
    <div>
    <h1>Ironhack Cinema</h1>
    <p>Click on the movie to check the showtimes!</p>
    </div>
    <br>
    {{#each movies}}
        <div class="movies-container">
           <img src="{{image}}">
           <p><{{title}}/p>
           <a href="movie/{{id}}">See more</a>
        </div>
    {{else}}
        There are no movies to show
    {{/each}}

</div>